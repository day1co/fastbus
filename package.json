{
  "name": "@day1co/fastbus",
  "version": "3.3.2",
  "description": "fast and simple pub/sub using redis",
  "url": "https://github.com/day1co/fastbus",
  "repository": "https://github.com/day1co/fastbus",
  "author": "iolo@day1company.co.kr",
  "license": "MIT",
  "main": "lib/index.js",
  "types": "lib/index.d.js",
  "scripts": {
    "build": "tsc",
    "clean": "npm-run-all -p clean:build clean:test clean:module",
    "clean:build": "rimraf ./lib",
    "clean:module": "rimraf ./node_modules",
    "clean:test": "rimraf ./coverage",
    "lint": "eslint --ext .js --ext .ts ./src",
    "prebuild": "npm-run-all -s clean:build lint",
    "prepublishOnly": "npm run build",
    "pretest": "npm-run-all -s clean:test lint",
    "serve": "echo no serve",
    "start": "npm run serve",
    "test": "jest --forceExit --detectOpenHandles --coverage --verbose ./src",
    "ci:test": "./ci_test.sh",
    "watch": "tsc -w"
  },
  "devDependencies": {
    "@day1co/eslint-config": "^1.3.1",
    "@day1co/prettier-config": "^1.2.2",
    "@day1co/tsconfig": "^1.3.0",
    "@tsconfig/node-lts": "^20.1.1",
    "@types/ioredis-mock": "^8.2.5",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.20",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "ioredis-mock": "^8.9.0",
    "jest": "^29.7.0",
    "npm-run-all": "^4.1.5",
    "pino": "^8.19.0",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@google-cloud/pubsub": "^4.3.2",
    "ioredis": "^5.3.2"
  }
}
