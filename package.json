{
  "name": "@day1co/fastbus",
  "version": "3.3.1",
  "description": "fast and simple pub/sub using redis",
  "url": "https://github.com/day1co/fastbus",
  "repository": "https://github.com/day1co/fastbus",
  "author": "iolo@day1company.co.kr",
  "license": "MIT",
  "main": "lib/index.js",
  "types": "lib/index.d.js",
  "scripts": {
    "build": "tsc",
    "clean": "npm-run-all -p clean:build clean:test clean:module",
    "clean:build": "rimraf ./lib",
    "clean:module": "rimraf ./node_modules",
    "clean:test": "rimraf ./coverage",
    "lint": "eslint --ext .js --ext .ts ./src",
    "prebuild": "npm-run-all -s clean:build lint",
    "prepublishOnly": "npm run build",
    "pretest": "npm-run-all -s clean:test lint",
    "serve": "echo no serve",
    "start": "npm run serve",
    "test": "jest --forceExit --detectOpenHandles --coverage --verbose ./src",
    "ci:test": "./ci_test.sh",
    "watch": "tsc -w"
  },
  "devDependencies": {
    "@day1co/eslint-config": "^1.0.2",
    "@day1co/prettier-config": "^1.0.0",
    "@day1co/tsconfig": "^1.2.1",
    "@tsconfig/node-lts": "^18.12.1",
    "@types/ioredis-mock": "^8.2.2",
    "@types/jest": "^29.5.3",
    "@types/node": "^18.16.19",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "ioredis-mock": "^8.8.1",
    "jest": "^29.6.1",
    "npm-run-all": "^4.1.5",
    "pino": "^8.14.1",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.1",
    "ts-jest": "^29.1.1",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@google-cloud/pubsub": "^3.7.0",
    "ioredis": "^5.3.2"
  }
}
